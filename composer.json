    "guzzlehttp/guzzle": "~6.0",
    "nette/utils": "~2.3",
    "sensio/framework-extra-bundle": "~3.0",
    "fzaninotto/faker": "^1.5",

    "trinity/logger": "dev-master",
    "trinity/bunny": "dev-master#9e01ca4a",
