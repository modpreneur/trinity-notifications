{
  "name": "trinity/notification",
  "description": "Trinity Notification bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Fajkus",
      "email": "fajkus@modpreneur.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "~6.0",
    "nette/utils": "~2.3",
    "sensio/framework-extra-bundle": "~3.0",
    "fzaninotto/faker": "^1.5",
    "doctrine/orm": "~2.5",
    "symfony/console": "~2.8 | ~3",
    "symfony/event-dispatcher": "~2.8 | ~3",
    "symfony/form": "~2.8 | ~3",

    "trinity/logger": "dev-master",
    "trinity/utils": "dev-master",
    "trinity/search": "dev-master",
    "trinity/settings": "dev-master",
    "trinity/bunny-bundle": "dev-master",
    "trinity/messages": "dev-master"
  },
  "require-dev": {
    "braincrafted/testing-bundle": "@stable",
    "friendsofsymfony/user-bundle": "dev-master",
    "sensio/generator-bundle": "~3.0",
    "phpunit/phpunit": "~5.4",
    "codeception/codeception" : "~2.2",
    "jms/serializer-bundle": "~1.1",
    "symfony/monolog-bundle": "~2.8"
  },

  "autoload": {
    "psr-4": { "Trinity\\NotificationBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },

  "minimum-stability": "stable",

  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
