{
  "name": "trinity/notification",
  "description": "Trinity Notification bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "ME",
      "email": "info@modpreneur.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/symfony": "~2.8 | ~3.0",
    "trinity/framework": "dev-master",
    "trinity/settings": "~1.0",
    "guzzlehttp/guzzle": "~6.0",
    "nette/utils": "~2.3",
    "sensio/framework-extra-bundle": "~3.0",
    "fzaninotto/faker": "^1.5",
    "trinity/logger": "dev-master"
  },
  "require-dev": {
    "braincrafted/testing-bundle": "@stable",
    "friendsofsymfony/user-bundle": "dev-master",
    "trinity/framework": "dev-master",
    "sensio/generator-bundle": "~3.0",
    "phpunit/phpunit": "~5.0",
    "codeception/codeception" : "~2.1",
    "jms/serializer-bundle": "~1.1",
    "symfony/monolog-bundle": "~2.8",
    "symfony/console": "~2.8| ~3.0"
  },

  "autoload": {
    "psr-4": { "Trinity\\NotificationBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },

  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
