{
  "name": "trinity/notification",
  "description": "Trinity Notification bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Fajkus",
      "email": "fajkus@modpreneur.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "doctrine/orm": "~2.5",
    "symfony/event-dispatcher": "2.7.*",
    "symfony/form": "2.7.*",
    "trinity/messages": "dev-sf27",
    "trinity/core-interfaces": "~1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.5",
    "braincrafted/testing-bundle": "~0.4",
    "friendsofsymfony/user-bundle": "~2.0",
    "sensio/generator-bundle": "~3.0",
    "phpunit/phpunit": "~6.0",
    "symfony/monolog-bundle": "~3.0",
    "phpstan/phpstan": "~0.7"
  },

  "autoload": {
    "psr-4": { "Trinity\\NotificationBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },

  "minimum-stability": "stable",

  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
