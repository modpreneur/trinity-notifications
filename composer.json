{
  "name": "trinity/notification",
  "description": "Trinity Notification bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Fajkus",
      "email": "fajkus@modpreneur.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~6.0",
    "nette/utils": "~2.3",
    "sensio/framework-extra-bundle": "~3.0",
    "doctrine/orm": "~2.5",
    "symfony/console": "~2.8 | ~3",
    "symfony/event-dispatcher": "~2.8 | ~3",
    "symfony/form": "~2.8 | ~3",

    "trinity/logger": "~1.0",
    "trinity/utils": "~1.0",
    "trinity/search": "dev-master",
    "trinity/settings": "~1.1",
    "trinity/messages": "dev-master",
    "trinity/core-interfaces": "~1.0",
    "trinity/core-entities": "~1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.5",
    "braincrafted/testing-bundle": "~0.4",
    "friendsofsymfony/user-bundle": "~2.0",
    "sensio/generator-bundle": "~3.0",
    "phpunit/phpunit": "~6.0",
    "codeception/codeception" : "~2.2",
    "jms/serializer-bundle": "~2.0",
    "symfony/monolog-bundle": "~3.0",
    "phpstan/phpstan": "~0.7"
  },

  "autoload": {
    "psr-4": { "Trinity\\NotificationBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },

  "minimum-stability": "stable",

  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
